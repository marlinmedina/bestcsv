import pandas as pd
from bs4 import BeautifulSoup
from pathlib import Path

# Path to uploaded HTML file
html_path = Path("rating_lift_drag.htm")  # Change if your file name differs

# Parse tables using pandas
tables = pd.read_html(str(html_path), flavor=["lxml", "bs4"])
print(f"Found {len(tables)} tables.")

# Inspect tables to find the right one
for i, df in enumerate(tables):
    print(f"Table {i}: Columns -> {df.columns.tolist()}")

# Assume the correct table is tables[0] (adjust if needed)
df = tables[0].copy()

# Normalize column names for easier matching
df.columns = df.columns.str.strip().str.lower()

# Select only the columns you need
wanted_cols = ["amb#", "company name", "rating lift/drag"]
selected_cols = [col for col in df.columns if any(key in col for key in ["amb", "company", "rating"])]
filtered_df = df[selected_cols]

# Save to CSV
filtered_df.to_csv("filtered_rating.csv", index=False)
print("Filtered CSV saved as filtered_rating.csv")
filtered_df.head()
